{
    "name": "overblog/thrift-bundle",
    "description": "OverBlog Thrift Bundle",
    "homepage": "https://github.com/overblog/ThriftBundle.git",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
          "name": "OverBlog",
          "homepage": "http://www.over-blog.com"
        }
    ],
    "autoload": {
        "psr-4": {"Overblog\\ThriftBundle\\": ""},
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config" : {
        "sort-packages": true
    },
    "require": {
        "php": "^5.4|~7.0",
        "apache/thrift": ">=0.9.1",
        "symfony/framework-bundle": "^2.3|^3.0",
        "symfony/options-resolver": "^2.3|^3.0",
        "symfony/process": "^2.3|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.1|^5.1",
        "ocramius/proxy-manager": "^1.0",
        "symfony/browser-kit": "^2.3|^3.0",
        "symfony/cache": "^3.1",
        "symfony/phpunit-bridge": "^2.3|^3.0"

    },
    "suggest": {
        "ocramius/proxy-manager": "Required if using client cache.",
        "symfony/cache": "Required if using client cache."
    }
}
